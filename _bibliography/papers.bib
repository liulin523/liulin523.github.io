	issn = {2169-9003},
	journal = {Journal of Geophysical Research-Earth Surface},
	keywords = {active-layer thickness satellite radar interferometry arctic coastal-plain thaw settlement climate soil usa summer earth field Geology},
	pages = {14},
	title = {InSAR measurements of surface deformation over permafrost on the North Slope of Alaska},
	type = {Journal Article},
	url = {<Go to ISI>://WOS:000281144800002},
	volume = {115},
