	keywords = {GPS uplift ice mass loss glaciers postglacial rebound 1240 Geodesy and Gravity Satellite geodesy results 0762 Cryosphere Mass balance 1211 Geodesy and Gravity Non-tectonic deformation 1218 Geodesy and Gravity 0720 Cryosphere Glaciers},
	number = {B9},
	pages = {B09405},
	title = {GPS measurements of crustal uplift near Jakobshavn Isbr{\ae} to glacial ice mass loss},
	type = {Journal Article},
	url = {http://dx.doi.org/10.1029/2010JB007490},
	volume = {115},
